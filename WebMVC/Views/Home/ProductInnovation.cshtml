@model IEnumerable<WebMVC.BLL.ProductInnvoationTable>

@{
    ViewBag.Title = "产品创新力";
}

<h2> </h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Stage)
        </th>
        @for (int i = 0; i < Model.Last().B.IIRC.Count; i++)
    {
        for (int j = 0; j < 3; j++)
        {
            <th>
                AR@(i + "" + j)
            </th>
    }
}



    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Stage)
        </td>

        @for (int i = 0; i < item.B.IIRC.Count; i++)
    {

        <td>
            @Math.Round(item.B.IIRC[i].M, 3)
        </td>
        <td>
            @Math.Round(item.B.IIRC[i].S, 3)
        </td>
        <td>
            @Math.Round(item.B.IIRC[i].J, 3)
        </td>
}

        }
    </tr>
}

</table>
